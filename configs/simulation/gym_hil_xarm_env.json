{
    "env": {
        "name": "gym_hil",
        "task": "XarmLiftGamepad-v0",
        "fps": 10,
        "features": {
            "observation.images.front": {
                "type": "VISUAL",
                "shape": [3, 84, 84]
            },
            "observation.images.wrist": {
                "type": "VISUAL",
                "shape": [3, 84, 84]
            },
            "observation.state": {
                "type": "STATE",
                "shape": [10]
            },
            "action": {
                "type": "ACTION",
                "shape": [4]
            }
        },
        "features_map": {
            "observation.images.front": "observation.images.front",
            "observation.images.wrist": "observation.images.wrist",
            "observation.state": "observation.state",
            "action": "action"
        },
        "robot": null,
        "teleop": null,
        "processor": {
            "control_mode": "gamepad",
            "gripper": {
                "use_gripper": true,
                "gripper_penalty": -0.02,
                "gripper_penalty_in_reward": false
            },
            "reset": {
                "reset_time_s": 2.0,
                "control_time_s": 15.0,
                "terminate_on_success": true
            },
            "inverse_kinematics": {
                "end_effector_step_sizes": {
                    "x": 0.025,
                    "y": 0.025,
                    "z": 0.025
                }
            },
            "observation": {
                "add_joint_velocity_to_observation": false,
                "add_current_to_observation": false,
                "add_ee_pose_to_observation": false,
                "display_cameras": true
            },
            "image_preprocessing": {
                "crop_params_dict": {},
                "resize_size": [84, 84]
            }
        }
    },
    "dataset": {
        "repo_id": null,
        "root": "/home/zekaijin/lerobot-hilserl-ufactory/datasets/xarm_sim_lift",
        "task": "XarmLift-v0",
        "num_episodes_to_record": 30,
        "replay_episode": 0,
        "push_to_hub": false
    },
    "mode": "record",
    "device": "cuda"
}

